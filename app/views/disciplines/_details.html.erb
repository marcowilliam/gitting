<a data-parent="#accordion" data-toggle="collapse" href="#collapseDescricao">Descrição
</a>
<p>
  <%= @discipline.discipline_description%>
</p>

<a data-parent="#accordion" data-toggle="collapse" href="#collapsePessoa">Pessoas</a>

<table class="table">
  <tbody>
    <tr>
      <td class="strong">
        <b>
          <p>Criador:
          </b>
        <%= @owner.username%>
        </p>
      </td>
    </tr>
    <tr>
      <td class="strong">
        <b>
          <p>Participantes:
          </b>
        </p>
      
        <% @participants.each do |participant|%>
        <p><%= participant.username %>
        </p>

        <% end %>
      </td>
    </tr>
  </tbody>
</table>

<% if current_user.id != @owner.id && @discipline.is_registered? == true %>
<a class="btn btn-primary btn-lg" href="/disciplines/new" role="button">Inscreva-se</a>
<% end %>